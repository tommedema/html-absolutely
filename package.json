{
  "name": "html-absolutely",
  "version": "1.2.0",
  "description": "Insert html and receive back html with all relative urls resolved to an absolute url.",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test",
    "e2e": "./node_modules/mocha/bin/mocha e2e --delay",
    "standard": "./node_modules/standard/bin/cmd.js --env mocha",
    "standard-fix": "npm run standard -- --fix",
    "docs": "./node_modules/documentation/bin/documentation.js lint src/** && ./node_modules/documentation/bin/documentation.js build src/** -f md >> README.md"
  },
  "keywords": [
    "html",
    "absolute",
    "relative",
    "resolve",
    "url",
    "href",
    "src",
    "stylesheet",
    "css"
  ],
  "author": "Tom Medema",
  "license": "MIT",
  "devDependencies": {
    "documentation": "^5.3.5",
    "loud-rejection": "^1.6.0",
    "mocha": "^4.0.1",
    "should": "^13.1.3",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "css-absolutely": "^1.2.0",
    "encodeurl": "^1.0.1",
    "srcset": "^1.0.0",
    "url-is-protoless": "^1.0.0"
  }
}
